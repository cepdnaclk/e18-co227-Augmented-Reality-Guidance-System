{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1659519321.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"id",
		"name",
		"rooms"
	],
	"properties": {
		"id": {
			"$id": "#root/id", 
			"title": "Id", 
			"type": "integer",
			"examples": [
				1
			],
			"default": 0
		},
		"name": {
			"$id": "#root/name", 
			"title": "Name", 
			"type": "string",
			"default": "",
			"examples": [
				"First Floor"
			],
			"pattern": "^.*$"
		},
		"rooms": {
			"$id": "#root/rooms", 
			"title": "Rooms", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/rooms/items", 
				"title": "Items", 
				"type": "string",
				"default": "",
				"examples": [
					"Computer Laboratory 2"
				],
				"pattern": "^.*$"
			}
		}
	}
}
